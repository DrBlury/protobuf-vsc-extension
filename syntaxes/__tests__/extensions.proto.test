// SYNTAX TEST "source.proto" "Extensions and reserved"

syntax = "proto2";

message Extendable {
  extensions 100 to 199;
//^^^^^^^^^^ keyword.control.extensions.proto
//           ^^^ constant.numeric.integer.proto
//               ^^ keyword.control.to.proto
//                  ^^^ constant.numeric.integer.proto

  extensions 1000 to max;
//^^^^^^^^^^ keyword.control.extensions.proto
//                   ^^^ keyword.control.max.proto

  reserved 2, 15, 9 to 11;
//^^^^^^^^ keyword.control.reserved.proto
//         ^ constant.numeric.integer.proto
//            ^^ constant.numeric.integer.proto
//                ^ constant.numeric.integer.proto
//                  ^^ keyword.control.to.proto
//                     ^^ constant.numeric.integer.proto

  reserved "foo", "bar";
//^^^^^^^^ keyword.control.reserved.proto
//         ^^^^^ string.quoted.double.proto
//                ^^^^^ string.quoted.double.proto
}

extend Extendable {
// <-- keyword.control.extend.proto
//     ^^^^^^^^^^ entity.name.type.proto
//                ^ punctuation.definition.block.proto

  optional string extended_field = 100;
}
