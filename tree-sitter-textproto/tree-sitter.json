{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "textproto",
      "camelcase": "Textproto",
      "scope": "source.textproto",
      "path": ".",
      "file-types": ["textproto", "textpb", "pbtext", "pbtxt"],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^textproto$"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Protocol Buffer Text Format grammar for tree-sitter",
    "authors": [
      {
        "name": "drblury"
      }
    ],
    "links": {
      "repository": "https://github.com/drblury/protobuf-vsc-extension"
    }
  },
  "bindings": {
    "c": true,
    "node": true
  }
}
